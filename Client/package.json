{
  "name": "hiv-platform",
  "version": "3.0.0",
  "description": "HIV Platform",
  "exports": "./index.js",
  "sideEffects": [
    "./src/utilities/*.js"
  ],
  "browserslist": [
    "> 5% and last 2 versions",
    "not IE 11"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "build-watch": "cross-env NODE_ENV=production webpack --watch",
    "build-stats": "cross-env NODE_ENV=production webpack --profile --json=./analyze/compilation-stats.json",
    "build-analyze": "webpack-bundle-analyzer ./analyze/compilation-stats.json ./build --mode 'static' --report ./analyze/report.html"
  },
  "repository": "https://github.com/nextpagesoft/hivPlatform",
  "author": "NextPage Software Daniel Lewandowski",
  "license": "EUPL-1.2",
  "engines": {
    "node": ">=13.11",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "5.0.0-alpha.126",
    "@mui/material": "^5.12.0",
    "@mui/system": "^5.12.0",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "file-saver": "^2.0.5",
    "jquery": "^3.6.4",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serializr": "^3.0.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@types/webpack-merge": "^5.0.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.1",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
